@model E_OneWeb.Models.ViewModels.RoomReservationUserVM

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var title = "Daftar Pinjam Ruangan";
}

<br />

<div class="row">
    <div class="col-md-12">
        <div class="col-md-6">
            <h2 class="text-primary">@title </h2>
        </div>
   
    </div>
</div>
<br />

<div class="p-4 border rounded" style="min-width:100px;overflow:scroll;">
    <table id="tblDataRoomReservationUser" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>
                    Nama Ruangan
                </th>
                <th>
                    Tanggal Pinjam
                </th>
                <th>
                    Jam Pinjam
                </th>
                <th>
                    Status
                </th>
                <th>
                    Keterangan
                </th>
                <th style="display:none">ID</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <partial name="_ValidationScriptsPartial" />	
	<script src="~/js/index_pinjamruanganuser.js"></script>
}