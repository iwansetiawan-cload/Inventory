@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var title = "Room List";
  
}
<br />
<div class="row">
    <div class="col-md-12">
        <div class="col-md-6">
            <h2 class="text-primary">@title </h2>
        </div>

    </div>
</div>

<br />
<div class="panel-body" style="min-width:100%;overflow:scroll">
    <div class="p-4 border rounded">
        <table id="tblDataRoomList" class="table table-striped table-bordered" style="width:100%">
            <thead class="thead-dark">
                <tr class="table-info">

                    <th>
                        Room Name
                    </th>
                    <th>
                        Location Name
                    </th>
                    <th>
                        Description
                    </th>
                    <th style="min-width:100px"></th>
                </tr>
            </thead>
        </table>
    </div>

    <br />


</div>

@section Scripts{
    <partial name="_ValidationScriptsPartial" />
	<script src="~/js/index_roomlist.js"></script>


}